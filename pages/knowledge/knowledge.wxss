.container {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
  color: rgb(7, 1, 1);
  font-size: 30rpx;
}

.leftheader1{
  margin-top: 10rpx;
  margin-bottom: 15rpx;
  margin-left: 10rpx;
  font-size: 50rpx;
}
.divLine{
  background: #E0E3DA;
  width: 100%;
  height: 2rpx;
}
.row_style{
  margin-top: 10rpx;
  margin-bottom: 15rpx;
  margin-left: 10rpx;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 80rpx;
  background: rgba(4, 53, 53, 0);
  align-items: center;
}
.row_style text{
  font-size: 30rpx;
}

.row_style .arrow{ 
  width: 10px;
  height: 10px;
  border-top: 2px solid #000;
  border-right: 2px solid #000;
  position: absolute;
  right: 30rpx;
  transform: rotate(45deg);
  margin-top:10rpx;
  align-items: center;
}
